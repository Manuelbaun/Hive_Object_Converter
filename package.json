{
  "name": "hive-object-converter",
  "displayName": "\"Hive Object Converter\"",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.44.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:hive-object-converter.convert_to_hive",
    "onCommand:hive-object-converter.get_generated_files"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "hive-object-converter.convert_to_hive",
        "title": "Convert To Hive"
      },
      {
        "command": "hive-object-converter.get_generated_files",
        "title": "Get Generated Files"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "hive-object-converter.convert_to_hive",
          "group": "aconvert_to_hive@1",
          "when": "resourceLangId == dart",
          "alt": "alt+z"
        },
        {
          "command": "hive-object-converter.get_generated_files",
          "group": "aconvert_to_hive@1",
          "when": "resourceLangId == dart",
          "alt": "alt+z"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/lodash": "^4.14.150",
    "@types/mkdirp": "^1.0.0",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/vscode": "^1.44.0",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.1",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "change-case": "^4.1.1",
    "child_process": "^1.0.2",
    "fs": "0.0.1-security",
    "lodash": "^4.17.15",
    "mkdir": "0.0.2"
  }
}
